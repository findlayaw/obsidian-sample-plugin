{
  "name": "obsidian-devtools-mcp",
  "version": "1.0.0",
  "description": "DevTools integration for Obsidian with programmatic access to developer console and elements inspection",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json"
  },
  "keywords": [
    "obsidian",
    "plugin",
    "devtools",
    "debugging",
    "development"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "5.29.0",
    "@typescript-eslint/parser": "5.29.0",
    "builtin-modules": "3.3.0",
    "esbuild": "0.17.3",
    "tslib": "2.4.0",
    "typescript": "4.7.4"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "latest",
    "electron": "^28.3.3",
    "electron-is-dev": "^3.0.1",
    "obsidian": "latest",
    "ws": "^8.18.1"
  }
}
